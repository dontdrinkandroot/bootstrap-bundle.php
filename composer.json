{
    "name": "dontdrinkandroot/bootstrap-bundle",
    "description": "Symfony integrations for Bootstrap",
    "type": "symfony-bundle",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Philip Washington Sorst",
            "email": "philip@sorst.net",
            "homepage": "https://sorst.net"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Dontdrinkandroot\\BootstrapBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require": {
        "php": "^8.1",
        "dontdrinkandroot/common": "^0.2",
        "symfony/config": "^5.4",
        "symfony/dependency-injection": "^5.4"
    },
    "require-dev": {
        "knplabs/knp-menu-bundle": "^3.2",
        "phpunit/phpunit": "^9.5.10",
        "symfony/browser-kit": "^5.4",
        "symfony/css-selector": "^5.4",
        "symfony/phpunit-bridge": "^5.4",
        "symfony/twig-bundle": "^5.4",
        "symfony/yaml": "^5.4",
        "doctrine/orm": "^2.10",
        "knplabs/knp-paginator-bundle": "^5.8"
    },
    "suggest": {
        "knplabs/knp-menu-bundle": "For menu integration"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
}
